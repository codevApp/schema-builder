{
    "name": "codevApp/schema-builder",
    "description": "Schema designer & migration generator for laravel & lumen",
    "version": "dev-master",
    "type": "library",
    "keywords": [
        "schema",
        "database",
        "schema builder",
        "schema design",
        "migration generator"
    ],
    "homepage": "https://github.com/Agontuk/schema-builder",
    "license": "MIT",
    "authors": [
        {
            "name": "Iftekhar Rifat",
            "email": "rifat662@gmail.com",
            "role": "Developer",
            "homepage": "https://github.com/Agontuk"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/codevapp/schema-builder"
        }
    ],
    "require": {
        "Agontuk/schema-builder": "dev-bugfix",
        "ext-zip": "*",
        "illuminate/filesystem": "^5.0.0",
        "illuminate/http": "^5.0.0",
        "illuminate/support": "^5.0.0",
        "league/flysystem": "^1.0",
        "league/flysystem-ziparchive": "^1.0"
    },
    "require-dev": {
        "phpmd/phpmd": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "Agontuk\\Schema\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Agontuk\\Schema\\SchemaServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
